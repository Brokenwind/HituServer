<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-cascade="all">
 <class name="com.zju.iot.entity.Password" table="password">
  <id column="password_id" name="passwordID" type="string">
   <generator class="uuid"/>
  </id>
  <property column="password" generated="never" lazy="false"
   name="password" not-null="true" type="string"/>
  <property column="update_date" generated="never" lazy="false"
   name="updateDate" type="timestamp"/>
 </class>
</hibernate-mapping>
